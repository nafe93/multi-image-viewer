{
  "name": "multi-image-viewer",
  "displayName": "Multi Image Viewer",
  "description": "Viewing identical images from different folders by index",
  "version": "0.0.1",
  "publisher": "nafe-kzir",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:multi-image-viewer.openFolders",
    "onCommand:multi-image-viewer.nextImage",
    "onCommand:multi-image-viewer.prevImage"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nafe93/multi-image-viewer.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "multi-image-viewer.openFolders",
        "title": "Open Image Folders"
      },
      {
        "command": "multi-image-viewer.nextImage",
        "title": "Next Image"
      },
      {
        "command": "multi-image-viewer.prevImage",
        "title": "Previous Image"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.25",
    "@types/vscode": "^1.90.0",
    "mocha": "^11.7.1",
    "typescript": "^5.4.0",
    "@vscode/vsce": "^3.6.0"
  }
}
